{
  "expo": {
    "name": "komdoritodo",
    "slug": "komdoritodo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/komdoritodo-icon.png",
    "splash": {
      "image": "./assets/komdoritodo-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.geekjeeyoung.komdoritodo"
    },
    "android": {
      "package": "com.geekjeeyoung.komdoritodo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "To Do Checklist App made with React Native ",
    "githubUrl": "https://github.com/hancandice/komdori-todo"
  }
}
